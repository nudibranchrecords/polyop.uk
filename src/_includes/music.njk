---
layout: base.njk
---

{% from "./symbol.njk" import getCurrentSymbol %}

<div class="page-wrap">
  <div class="reading-container">
    <h1>
      <span class="material-symbols-outlined">{{ getCurrentSymbol(page.url, meta) }}</span>
      {{ title }}
    </h1>
  </div>
  <section>

    <div class='release-list'>
      {% for release in collections.releases | reverse %}
        <div class='release-item'>
          <div class='release-thumb shimmer'>
            <iframe style="border: 0; width: 100%; height: 100%;" src="https://bandcamp.com/EmbeddedPlayer/{{release.data.type}}={{release.data.bandcampid}}/size=large/bgcol=111111/linkcol=0687f5/minimal=true/transparent=true/" seamless></iframe>
          </div>
        </div>
      {% endfor %}
    </section>

